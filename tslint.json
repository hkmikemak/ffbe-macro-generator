{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-immutable"
    ],
    "jsRules": {},
    "rules": {
      "max-line-length": false,
      "object-literal-sort-keys": [true, "ignore-case", "shorthand-first"],
      "ordered-imports": [
        true,
        {
          "import-sources-order": "lowercase-last",
          "named-imports-order": "lowercase-first"
        }
      ]
    },
    "rulesDirectory": []
}
